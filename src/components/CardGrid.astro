---
import Card from "./Card.astro";
const { contents, link } = Astro.props;
---

<ul>
    {
        contents.map((content: any) => <Card content={content} link={link} />)
    }
</ul>

<style>
    ul {
        display: flex;
        flex-wrap: wrap;
    }
    a:visited{
        color: inherit;
    }
</style>
